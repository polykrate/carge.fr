{
  "type": "object",
  "required": ["nationalDistribution"],
  "properties": {
    "nationalDistribution": {
      "type": "object",
      "required": [
        "distributorName",
        "transferDate",
        "fromEntity",
        "batchNumber",
        "bottleNumberRange",
        "bottlesInLot",
        "volumeTotal",
        "storageConditions",
        "qualityCheck",
        "transportMethod"
      ],
      "properties": {
        "distributorName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 200,
          "description": "Name of the national distributor (FIRST FIELD)"
        },
        "transferDate": {
          "type": "string",
          "format": "date",
          "description": "Date of transfer (YYYY-MM-DD)"
        },
        "fromEntity": {
          "type": "string",
          "minLength": 1,
          "maxLength": 200,
          "description": "Name of the sending entity"
        },
        "batchNumber": {
          "type": "string",
          "pattern": "^[A-Z0-9-]+$",
          "minLength": 1,
          "maxLength": 50,
          "description": "Batch number being transferred"
        },
        "bottleNumberRange": {
          "type": "string",
          "pattern": "^[0-9]+-[0-9]+$",
          "description": "Range of bottle numbers in this transfer"
        },
        "bottlesInLot": {
          "type": "integer",
          "minimum": 1,
          "maximum": 1000000,
          "description": "Number of bottles in this lot"
        },
        "volumeTotal": {
          "type": "number",
          "minimum": 0.1,
          "maximum": 1000000,
          "description": "Total volume in liters"
        },
        "storageConditions": {
          "type": "string",
          "minLength": 1,
          "maxLength": 300,
          "description": "Storage conditions at destination"
        },
        "qualityCheck": {
          "type": "string",
          "enum": ["conforme", "non-conforme", "pending"],
          "description": "Quality verification status"
        },
        "qualityInspection": {
          "type": "string",
          "maxLength": 300,
          "description": "Details of quality inspection performed"
        },
        "transportMethod": {
          "type": "string",
          "minLength": 1,
          "maxLength": 200,
          "description": "Method of transportation used"
        },
        "customsDocLicense": {
          "type": "string",
          "maxLength": 100,
          "description": "Customs or license document number"
        },
        "customsDocCid": {
          "type": "string",
          "pattern": "^baf[a-z0-9]+$",
          "description": "IPFS CID of customs document"
        },
        "transferNotes": {
          "type": "string",
          "maxLength": 500,
          "description": "Additional notes about the transfer"
        }
      }
    }
  }
}

